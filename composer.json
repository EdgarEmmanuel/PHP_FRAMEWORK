{
    "name": "emmanuel/edgar",
    "authors": [
        {
            "name": "Edgar",
            "email": "mmnledgar@gmail.com"
        }
    ],
    "config": {

        "platform-check": false
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "src/Framework",
            "App\\": "app/",
            "Tests\\Framework\\": "tests/Framework"
        }
    },
    "scripts": {
        "phpunit": "./vendor/bin/phpunit",
        "phpSniffThisFile": "./vendor/bin/phpcs ./src",
        "phpFixAllFile": "./vendor/bin/phpcbf ./src"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "guzzlehttp/psr7": "^2.1",
        "http-interop/response-sender": "^1.0",
        "squizlabs/php_codesniffer": "*",
        "zendframework/zend-expressive-fastroute": "^0.3.0",
        "twig/twig": "^3.3",
        "php-di/php-di": "^6.3",
        "robmorgan/phinx": "^0.12.9",
        "symfony/console": "5.4.2",
        "fakerphp/faker": "^1.18"
    }
}
